{% extends "./page.njk" %}

{% block page_content %}
  {% if title %}
    <h1 class="govuk-fieldset__heading">{{ title }}</h1>
  {% endif %}
  {% if line1 %}
    <div class="govuk-hint">{{ line1 }}</div>
  {% endif %}
  {% if line2 %}
    <p class="govuk-body">{{ line2 }}</p>
  {% endif %}
{% endblock %}