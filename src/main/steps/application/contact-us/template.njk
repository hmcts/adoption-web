{% extends "common/page.njk" %}

{% block content %}
  <h1 class="govuk-heading-l">{{ title }}</h1>
  <div class="govuk-details__text">
    <h2 class="govuk-heading-m">{{sendUsAMessageHeading}}</h2>
      {% if laPortal == true %}
        <p class="govuk-body">{{ findCourtTribunalDetailsLA | safe }}</p>
      {% else %}
        <p class="govuk-body">{{ findCourtTribunalDetails | safe }}</p>
        <p class="govuk-body">{{ askIfUnsure }}</p>
      {% endif %}
  </div>
{% endblock %}
