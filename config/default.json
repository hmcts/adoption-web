{
  "useCSRFProtection": true,
  "security": {
    "referrerPolicy": "origin"
  },
  "appInsights": {
    "instrumentationKey": false
  },
  "session": {
    "cookieName": "SESSION_ID"
  },
  "http": {
    "timeout": 60000
  },
  "requestRetry": {
    "maxAttempts": 3
  },
  "oauth": {
    "clientId": "cmc_citizen"
  },
  "idam": {
    "authentication-web": {
      "url": "http://localhost:9002"
    },
    "api": {
      "url": "http://localhost:5000"
    },
    "service-2-service-auth": {
      "url": "http://localhost:4552",
      "microservice": "cmc"
    }
  },
  "secrets": {
    "cmc": {
      "cmc-s2s-secret": "AAAAAAAAAAAAAAAA",
      "staff-email": "CONTACT_EMAIL",
      "citizen-oauth-client-secret": "12345678",
      "encryptionKey": "not-a-real-32-bit-encryption-key",
      "AppInsightsInstrumentationKey": "app_insights_instrumental_key"
    }
  }
}
